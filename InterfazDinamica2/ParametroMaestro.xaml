<UserControl x:Class="InterfazDinamica2.ParametroMaestro"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InterfazDinamica2"
             mc:Ignorable="d" Background="White"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        
        <TextBlock x:Name="lblNomM" HorizontalAlignment="Left" Margin="2,23,0,0" TextWrapping="Wrap" Text="Nombre" VerticalAlignment="Top"/>
        <TextBox x:Name="txtNomM" HorizontalAlignment="Left" Height="23" Margin="85,25,0,0" TextWrapping="Wrap" Text="..." VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="lblMatrM" HorizontalAlignment="Left" Margin="2,69,0,0" TextWrapping="Wrap" Text="Matricula" VerticalAlignment="Top"/>
        <TextBox x:Name="txtMatrM" HorizontalAlignment="Left" Height="23" Margin="85,69,0,0" TextWrapping="Wrap" Text="..." VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="lblPrepM" HorizontalAlignment="Left" Margin="2,113,0,0" TextWrapping="Wrap" Text="Preparatoria" VerticalAlignment="Top"/>
        <TextBox x:Name="txtPrepM" HorizontalAlignment="Left" Height="23" Margin="85,113,0,0" TextWrapping="Wrap" Text="..." VerticalAlignment="Top" Width="120"/>

    </Grid>
</UserControl>
